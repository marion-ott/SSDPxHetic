{
	"name": "server",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"start": "node src/index.js --ext js,graphql --exec babel-node",
		"dev": "nodemon src/index.js --ext js,graphql --exec babel-node",
		"task": "nodemon src/utils/tasks.js --ext js,graphql --exec babel-node",
		"deploy": "prisma deploy -e config/dev.env && prisma generate -e config/dev.env",
		"token": "prisma token -e config/dev.env",
		"delete": "prisma delete -f -e config/dev.env",
		"data": "nodemon data/index.js --ext js,graphql --exec babel-node"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"graphql-yoga": "^1.18.3",
		"jsonwebtoken": "^8.5.1",
		"lodash.deburr": "^4.1.0",
		"lodash.shuffle": "^4.2.0",
		"moment": "^2.26.0",
		"prisma-client-lib": "^1.34.10"
	},
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.9.0",
		"@babel/node": "^7.8.7",
		"@babel/plugin-transform-spread": "^7.8.3",
		"@babel/preset-env": "^7.9.5",
		"axios": "^0.19.2",
		"nodemon": "^2.0.3",
		"prettier": "^1.19.1",
		"prisma": "^1.34.10"
	}
}
